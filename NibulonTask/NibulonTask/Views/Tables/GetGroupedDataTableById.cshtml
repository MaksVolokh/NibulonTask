@model NibulonBLL.Dto.GroupedDataTableDTO

@{
    Layout = "_Layout";
}

<h2>Grouped Data Table</h2>

<div class="container">
    <div class="table-responsive">
        <table class="table table-striped table-bordered table-hover">
            <thead class="thead-dark">
                <tr>
                    <th>Id</th>
                    <th>Record Number</th>
                    <th>Date</th>
                    <th>Department Code</th>
                    <th>Harvest Year</th>
                    <th>Contractor</th>
                    <th>Name</th>
                    <th>Unique Contract Number</th>
                    <th>Item Code</th>
                    <th>Price</th>
                    <th>Net Quantity</th>
                    <th>Direction</th>
                    <th>Moisture</th>
                    <th>Impurity</th>
                    <th>Contamination</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>@Model.Id</td>
                    <td>@Model.RecordNumber</td>
                    <td>@Model.Date</td>
                    <td>@Model.DepartmentCode</td>
                    <td>@Model.HarvestYear</td>
                    <td>@Model.Contractor</td>
                    <td>@Model.Name</td>
                    <td>@Model.UniqueContractNumber</td>
                    <td>@Model.ItemCode</td>
                    <td>@Model.Price</td>
                    <td>@Model.NetQuantity</td>
                    <td>@Model.Direction</td>
                    <td>@Model.Moisture</td>
                    <td>@Model.Impurity</td>
                    <td>@Model.Contamination</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>



